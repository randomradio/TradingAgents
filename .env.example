ALPHA_VANTAGE_API_KEY=alpha_vantage_api_key_placeholder
OPENAI_API_KEY=openai_api_key_placeholder

# Optional: other LLM providers (set if using Anthropic/Google)
# ANTHROPIC_API_KEY=your_anthropic_api_key
# GOOGLE_API_KEY=your_google_api_key

# Optional: use any OpenAI-compatible LLM provider
# TRADINGAGENTS_LLM_PROVIDER=custom
# TRADINGAGENTS_BASE_URL=https://api.openai.com/v1
# TRADINGAGENTS_API_KEY=your_api_key
# TRADINGAGENTS_DEEP_THINK_LLM=o4-mini
# TRADINGAGENTS_QUICK_THINK_LLM=gpt-4o-mini

# Optional: separate embedding config (defaults to LLM provider settings)
# TRADINGAGENTS_EMBEDDING_MODEL=text-embedding-3-small
# TRADINGAGENTS_EMBEDDING_BASE_URL=https://api.openai.com/v1
# TRADINGAGENTS_EMBEDDING_API_KEY=your_embedding_api_key

# Optional: output directory
# TRADINGAGENTS_RESULTS_DIR=./results

# Optional: Telegram bot integration
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token
# TELEGRAM_ALLOWED_USERS=comma_separated_user_ids_or_empty_for_all
